{	
  "razor-inject": {
		"prefix": "razor-inject",
		"body": [
      "@inject ${1:Type} ${2:variableName}"
    ],
		"description": "Generates an @inject block."
	},
	"razor-environment": {
		"prefix": "razor-environment",
		"body": [
      "  <environment names=\"$0\">",
      "  </environment>"
    ],
		"description": "Generates an Environment block."
	},
	"razor-raw": {
		"prefix": "razor-raw",
		"body": [
      "@Html.Raw($0)"
    ],
		"description": "Generates an @Html.Raw block."
	}
}